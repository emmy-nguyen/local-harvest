{
  "name": "local-harvest-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:server": "nodemon --watch 'src/**/*.ts' --exec ts-node src/server.ts",
    "build:ts": "tsc",
    "build:assets": "npx cpx2 \"src/**/*.{ejs,css,prisma,sql}\" dist",
    "build": "npm run build:ts && npm run build:assets",
    "start:watcher": "browser-sync start --proxy 'localhost:8000' --files 'src/**/*.ejs, src/**/*.css' --port 3000",
    "reload-browser": "browser-sync reload"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.13.0",
    "@types/express-session": "^1.18.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "multer": "^1.4.5-lts.1",
    "randomuuid": "^1.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.7",
    "browser-sync": "^3.0.2",
    "cpx2": "^7.0.1",
    "nodemon": "^3.1.0",
    "prisma": "^5.13.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
